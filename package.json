{
  "name": "aplikasi-inventaris",
  "version": "1.0.0",
  "description": "Web-based inventory app",
  "main": "index.js",
  "scripts": {
    "start": "node ./src/index.js",
    "dev": "nodemon ./src/index.js",
    "lummox": "javascript-obfuscator src --output _build --compact false",
    "copy": "node copy",
    "build": "rimraf _build && npm run lummox && npm run copy",
    "serve": "node ./_build/index.js"
  },
  "author": "valenoirs",
  "license": "MIT",
  "dependencies": {
    "connect-flash": "^0.1.1",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-ejs-layouts": "^2.5.1",
    "express-session": "^1.17.3",
    "method-override": "^3.0.0",
    "mongoose": "^6.3.8",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "fs-extra": "^11.1.1",
    "javascript-obfuscator": "^4.1.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5"
  }
}
