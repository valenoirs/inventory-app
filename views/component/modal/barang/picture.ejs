<% barangs.forEach(barang => { %>
<div
  class="modal fade"
  id="picture<%= barang.id %>"
  tabindex="-1"
  aria-labelledby="pictureLabel"
  aria-hidden="true"
>
  <div class="modal-dialog modal-dialog-scrollable modal-dialog-centered">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title text-success" id="pictureLabel">
          <i class="bi bi-chevron-right"></i> <%= barang.name %> - <%=
          barang.code %>
        </h5>
        <button
          type="button"
          class="btn-close"
          data-bs-dismiss="modal"
          aria-label="Close"
        ></button>
      </div>
      <div class="modal-body">
        <img
          src="<%= barang.picture %>"
          alt="<%= barang.name %>-<%= barang.code %>"
          width="100%"
        />
      </div>
    </div>
  </div>
</div>
<% }) %>
